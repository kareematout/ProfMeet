<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="s25.cs151.application.CourseViewerController">
    <TableView fx:id="courseTable" layoutX="17.0" layoutY="53.0" prefHeight="406.0" prefWidth="610.0">
        <columns>
            <TableColumn fx:id="courseCodeColumn" prefWidth="180.0" text="Course Code" />
            <TableColumn fx:id="courseNameColumn" prefWidth="300.0" text="Course Name" />
            <TableColumn fx:id="sectionNumberColumn" prefWidth="120.0" text="Section" />
        </columns>
    </TableView>
    <HBox layoutX="17.0" prefHeight="50.0" prefWidth="488.0">
        <children>
            <Label prefHeight="38.0" prefWidth="218.0" text="Stored Courses">
                <font>
                    <Font name="System Bold" size="24.0" />
                </font>
                <HBox.margin>
                    <Insets top="5.0" />
                </HBox.margin>
            </Label>
            <Button mnemonicParsing="false" onAction="#goToHome" style="-fx-background-color: #6C47FF;" text="Home" textFill="WHITE">
                <HBox.margin>
                    <Insets top="12.0" />
                </HBox.margin>
            </Button>
        </children>
    </HBox>
</AnchorPane>
